<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xnzygxpt.dating.dao.dao.DatingMapper">

	<insert id="add" parameterType="Dating" useGeneratedKeys="true"
		keyProperty="dating">
		insert into
		dating() values
		()
	</insert>
	<update id="update" parameterType="Dating">
		update dating set
		professional=#{professional},business=#{business},state=#{state},signature=#{signature},hometown=#{hometown}
		where dating=#{dating}
	</update>
	<select id="queryByID" parameterType="int" resultType="Dating">
		select
		*
		from dating where dating=#{dating}
	</select>
</mapper>